{
  "name": "aldeebhasan/naive-crud",
  "description": "A lightweight package to handle the laravel crud operations",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "cache",
    "laravel crud",
    "crud",
    "crud operations"
  ],
  "authors": [
    {
      "name": "Hasan Deeb",
      "email": "aldeeb.91@gmail.com",
      "homepage": "https://hasan-deeb.com"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Aldeebhasan\\NaiveCrud\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Aldeebhasan\\NaiveCrud\\Test\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0.1",
    "orchestra/testbench": "9.x-dev"
  },
  "extra": {
    "laravel": {
      "providers": [
        "NCProvider"
      ],
      "aliases": {
      }
    }
  }
}
